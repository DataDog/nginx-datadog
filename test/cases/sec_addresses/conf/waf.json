{
  "version": "2.1",
  "metadata": {
    "rules_version": "1.2.6"
  },
  "rules": [
    {
      "id": "match_keys",
      "name": "Match keys",
      "tags": {
        "type": "security_scanner",
        "category": "attack_attempt"
      },
      "conditions": [
        {
          "parameters": {
            "inputs": [
              {
                "address": "server.request.query"
              },
              {
                "address": "server.request.cookies"
              },
              {
                "address": "server.request.headers.no_cookies"
              },
              {
                "address": "server.response.headers.no_cookies"
              },
              {
                "address": "server.request.body"
              }
            ],
            "regex": "^(?:%(?:a)?)?matched[ -]key(?:%(?:a)?)?$"
          },
          "operator": "match_regex"
        }
      ],
      "transformers": [
        "keys_only"
      ]
    },
    {
      "id": "match_values",
      "name": "Match values",
      "tags": {
        "type": "security_scanner",
        "category": "attack_attempt"
      },
      "conditions": [
        {
          "parameters": {
            "inputs": [
              {
                "address": "server.request.query"
              },
              {
                "address": "server.request.cookies"
              },
              {
                "address": "server.request.headers.no_cookies"
              },
              {
                "address": "server.response.headers.no_cookies"
              },
              {
                "address": "server.request.body"
              }
            ],
            "regex": "^(?:%(?:a)?)?matched value(?:%(?:a)?)?$$"
          },
          "operator": "match_regex"
        }
      ],
      "transformers": [
        "values_only"
      ]
    },
    {
      "id": "partial_match_values",
      "name": "Partially match values",
      "tags": {
        "type": "security_scanner",
        "category": "attack_attempt"
      },
      "conditions": [
        {
          "parameters": {
            "inputs": [
              {
                "address": "server.request.uri.raw"
              }
            ],
            "regex": ".*matched\\+partial\\+value.*"
          },
          "operator": "match_regex"
        }
      ],
      "transformers": [
        "values_only"
      ]
    },
    {
      "id": "5xx",
      "name": "Match 5xx responses",
      "tags": {
        "type": "security_scanner",
        "category": "attack_attempt"
      },
      "conditions": [
        {
          "parameters": {
            "inputs": [
              {
                "address": "server.response.status"
              }
            ],
            "regex": "5\\d{2}"
          },
          "operator": "match_regex"
        }
      ],
      "transformers": [
        "values_only"
      ]
    },
    {
      "id": "put",
      "name": "Match PUT requests",
      "tags": {
        "type": "security_scanner",
        "category": "attack_attempt"
      },
      "conditions": [
        {
          "parameters": {
            "inputs": [
              {
                "address": "server.request.method"
              }
            ],
            "regex": "PUT"
          },
          "operator": "match_regex"
        }
      ],
      "transformers": [
        "values_only"
      ]
    },
    {
      "id": "match_ip",
      "name": "Matches IP address 1.2.3.4",
      "tags": {
        "type": "security_scanner",
        "category": "attack_attempt"
      },
      "conditions": [
        {
          "parameters": {
            "inputs": [
              {
                "address": "http.client_ip"
              }
            ],
            "regex": "1\\.2\\.3\\.4|8\\.8\\.8\\.8|fe80::1"
          },
          "operator": "match_regex"
        }
      ],
      "transformers": [
        "values_only"
      ]
    }
  ]
}
